{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "pipeline": {
    "dev": {
      "dependsOn": ["^build:dev"],
      "env": ["PORT"]
    },
    "dev:push": {
      "dependsOn": ["database#db:push", "^build:dev"],
      "env": ["PORT"]
    },
    "deploy": {
      "env": ["PORT"]
    }
  }
}
